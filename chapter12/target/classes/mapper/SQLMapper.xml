<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper>
   <!--获取登录接口case-->
    <select id="loginCase" parameterType="Integer" resultType="com.course.model.LoginCase">
        select * from loginCase
        where id=#{}
    </select>
    <!--添加用户接口case-->
    <select id="addUserCase" parameterType="integer" resultType="com.course.model.AddUserCase">
        select * from addUserCase where id=#{}
    </select>
    <!--获取用户信息接口case-->
    <select id="getUserInfoCase" parameterType="integer" resultType="com.course.model.GetUserInfoCase">
        select * from getUserInfoCase where id=#{}
    </select>
    !--获取用户列表接口case-->
    <select id="getUserInfoList" parameterType="integer" resultType="com.course.model.GetUserListCase">
         select * from getUserInfoList where id=#{}
    </select>

    <select id="updateUserInfo" parameterType="integer" resultType="com.course.model.UpdateUserInfoCase">
        select * from updateUserInfoCase where id=#{}
    </select>
</mapper>